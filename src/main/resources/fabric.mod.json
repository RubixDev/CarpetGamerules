{
  "schemaVersion": 1,
  "id": "carpetgamerules",
  "version": "${version}",

  "name": "Carpet Gamerules",
  "description": "Adds all vanilla gamerules to the carpet settings",
  "authors": [
    "RubixDev"
  ],
  "contact": {
    "homepage": "https://github.com/RubixDev/CarpetGamerules",
    "sources": "https://github.com/RubixDev/CarpetGamerules",
    "issues": "https://github.com/RubixDev/CarpetGamerules/issues"
  },

  "license": "GPL-3.0-only",
  "icon": "assets/carpetgamerules/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "de.rubixdev.carpetgamerules.CarpetGamerulesServer"
    ]
  },
  "mixins": [
    "carpetgamerules.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabric_loader}",
    "carpet": "${carpet_dependency}",
    "minecraft": "${minecraft_dependency}"
  },
  "suggests": {
  },
  "custom": {
    "modmenu": {
      "parent": "carpet"
    }
  }
}
